{
  "compilerOptions": {
    "target": "es2017",
    "moduleResolution": "node",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "rootDir": "src/working",
    "outDir": "dist",
    "lib": [
      "esnext",
      "dom"
    ],
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
  }
}